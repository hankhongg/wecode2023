#include <iostream>
#include <string.h>
using namespace std;
#define MAX 300

void XoaKhoangTrangDu_SauDauCham(char *s)
{
    char *vt = strstr(s, ".");
    do{

        if (*(vt+1)==' ')
        {
            if (*(vt+2)==' ')
            {
                memmove(vt+1, vt+2, strlen(vt+2)+1);
                vt = strstr(vt, ".");
            }
            else vt = strstr(vt+1, ".");
        }
        else vt = strstr(vt+1, ".");
    } while (vt!=NULL);
}

int main() {
	char s[MAX];
	cin.getline(s, MAX);
	cout << s << endl;
	char s1[MAX];
	strcpy(s1, s);
	XoaKhoangTrangDu_SauDauCham(s1);
	cout << s1 << endl;
	return 0;
}
