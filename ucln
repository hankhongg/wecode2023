using namespace std;
void intput(int &n)
{
    cin >> n;
}

int input()
{
    int m;
    cin >> m;
    return m;
}

int gcd(int &a,int &b)
{
    if (a==0 || b==0)
    {
        return a+b;
    }
    while (a!=b)
    {
        if (a>b){
            a-=b;
        }
        else b-=a;
    }
    return a;
}
