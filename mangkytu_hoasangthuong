#include <iostream>
#define MAXN 200
using namespace std;

void Input(int &n)
{
   cin >> n;
}
void NhapMang(char a[], int n)
{
    for (int i=0; i<n; i++)
    {
        cin >> a[i];
    }
}

void XuatMang(char a[], int n)
{
    for (int i=0; i<n; i++)
    {
        if (a[i]>96 && a[i] <123)
        {
            a[i]-=32;
        }
        else if ((int)a[i]>64 && a[i] <91)
        {
            a[i]+=32;
        }

        cout << (char)a[i] << "\t";
    }
}

int main() {
    char a[MAXN];
    int n;
    Input(n); // Yêu cầu: n được nhập trong hàm Input!
    NhapMang(a, n);
    XuatMang(a, n);
    return 0;
}
