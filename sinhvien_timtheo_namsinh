#include <iomanip>
#include <iostream>
#include <stdio.h>
#include <string.h>
#include <string>
#include <math.h>
using namespace std;
#define MAXN 100

struct SinhVien {
    char MASV[10];
    char HoTen[100];
    char NgaySinh[12];
    char GioiTinh;
    float DiemToan, DiemLy, DiemHoa, DTB;
};


void NhapD(SinhVien &a)
{
    cin.getline(a.MASV, MAXN);
    cin.getline(a.HoTen, MAXN);
    cin.getline(a.NgaySinh, MAXN);
    cin >> a.GioiTinh >> a.DiemToan >> a.DiemLy >> a.DiemHoa;
    cin.ignore();
}

void Nhap(SinhVien a[], int &n)
{

    cin >> n;
    cin.ignore();
    for (int i=0; i<n; i++)
    {
        NhapD(a[i]);
    }
}

char* bd(SinhVien &a)
{
    char *s;
    s = new char[MAXN];
    int j=0;
    for (int i=6; i<10; i++)
    {
        s[j++] = a.NgaySinh[i];
    }
    s[j]='\0';
    return s;
}

int laynamsinh(SinhVien &a)
{
    char *s;
    s = new char[MAXN];;
    s = bd(a);
    int ns=0;
    for (int i=0; i<4; i++)
    {
        ns+=((int)(s[i]-48))*pow(10, 3-i);
    }
    return ns;
}

void Xuat(SinhVien a)
{
    cout << a.MASV << "\t" << a.HoTen << "\t" << a.NgaySinh << "\t" << a.GioiTinh << "\t" << a.DiemToan << "\t" << a.DiemLy
    << "\t" << a.DiemHoa << "\t" << setprecision(3) << (a.DiemToan+a.DiemLy+a.DiemHoa)/3 << endl;
}

void TimTheoNamSinh(SinhVien a[], int &n, int &namsinh)
{
    int ns[MAXN];
    for (int i=0; i<n; i++)
    {
        ns[i] = laynamsinh(a[i]);
    }

    for (int i=0; i<n; i++)
    {
        if (ns[i]==namsinh) Xuat(a[i]);
    }

}

int main() {
    SinhVien A[MAXN];
    int n, namsinh;
    std::cin >> namsinh;
    Nhap(A, n);
    TimTheoNamSinh(A, n, namsinh);

    return 0;
}
