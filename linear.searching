void NhapMang(int a[], int &n)
{
    std::cin >> n;
    for (int i=0; i<n; i++)
    {
        std::cin >> a[i];
    }
}

int LinearSearch(int a[], int &n, int &x)
{
    for (int i=0; i<n; i++)
    {
        if (a[i] == x)
        {
            return 1;
            break;
        }

    }
    return -1;
}
