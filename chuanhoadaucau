#include <iostream>
#include <string.h>
using namespace std;
#define MAX 300

void ThemCach(char *s)
{
    char *vt = strstr(s, ".");
    while(1)
    {
        if (vt==NULL) return;
        if (*(vt+1)!=' ')
        {
            memmove(vt+1, vt, strlen(vt)+1);
            *(vt+1)=' ';
        }
        vt = strstr(vt+1, ".");
    }
}

void XoaKhoangTrangDu_TruocDauCham(char *s)
{
    while (1)
    {
        char *vt = strstr(s, " .");
        if (vt ==NULL) return;
        memmove(vt, vt+1, strlen(vt)+1);
    }
}



void ChuanHoaDauCau(char s[])
{
    ThemCach(s);
    XoaKhoangTrangDu_TruocDauCham(s);

}

int main() {
	char s[MAX];
	cin.getline(s, MAX);
	cout << s << endl;
	char s1[MAX];
	strcpy(s1, s);
    ChuanHoaDauCau(s1);
	cout << s1 << endl;
	return 0;
}
