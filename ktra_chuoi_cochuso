void NhapChuoi(char chuoi[])
{
    cin.getline(chuoi, MAX);
}

int KTC(char chuoi[])
{
    int k=0;
    while (chuoi[k]!='\0') k++;
    for (int i=0; i<k; i++)
    {

        if (chuoi[i]>47 && chuoi[i]<58)
        {
            return 1;
        }
    }
    return 0;
}

int main()
{
    char chuoi[MAX];
    NhapChuoi(chuoi);

    if (KTC(chuoi)==1)
    {
        cout << "CHUOI KHONG HOP LE.";
    }
    else cout << chuoi;
}
