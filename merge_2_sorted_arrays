#include <iostream>
#include <climits>
#include <math.h>
#define MAXN 150000

void NhapMang(int A[], int &N) {
    std::cin >> N;
    for (int i = 0; i < N; i++)
        std::cin >> A[i];
}

void merge_2_Sorted_subArray(int a[], int &l, int&m, int &r)
{
    int b[MAXN], c[MAXN];
    int nb=0, nc=0, n1=0, n2=0;
    int k=0;
    m++; r++;
    for (int i=l; i<m; i++)
    {
        b[n1++]=a[i];
    }
    for (int i=m; i<r; i++)
    {
        c[n2++]=a[i];
    }

    while (nb<n1 && nc<n2)
    {
        if(b[nb]<c[nc])
        {
            a[k++]=b[nb++];
        }
        else a[k++]=c[nc++];
    }

    while (nb<n1) a[k++] = b[nb++];

    while (nc<n2) a[k++] = c[nc++];
}

void XuatMang(int A[], const int &N) {
    std::cout << N << std::endl;
    for (int i = 0; i < N; i++)
        std::cout << A[i] << " ";
}

int main() {
    int a[MAXN], n;

    NhapMang(a, n);

	int l=0, r=n-1, middle =(l+r)/2;
    merge_2_Sorted_subArray(a, l, middle, r);

    XuatMang(a, n);

    return 0;
}
