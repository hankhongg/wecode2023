#include <iostream>

using namespace std;

int kt(int &n)
{
    int k;
    if (n>=0)
    {
        k=n%360;
        if (k>=0 && k<90) return 1;
        else if (k>=90 && k<180) return 2;
        else if (k>=180 && k<270) return 3;
        else if (k>=270 && k<360) return 4;
    }
    else
    {
        k=-n%360;
        if (k>0 && k<=90) return 4;
        else if (k>90 && k<=180) return 3;
        else if (k>180 && k<=270) return 2;
        else if (k>270 && k<=360) return 1;
    }



}

int main()
{
    int n;
    cin >> n;
    cout << kt(n);
    return 0;
}
