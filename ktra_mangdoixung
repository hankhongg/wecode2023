#include <iostream>
#define MAXN 100


void NhapMang(int a[], int &n)
{
    std::cin >> n;
    for (int i=0; i<n; i++)
    {
        std::cin >> a[i];
    }
}

bool is_MangDoiXung(int a[], int &n)
{
    if (n==1)
    {
        return true;
    }

    int k=n/2;
    int m=0;

    for (int i=0; i<k; i++)
    {
        {
            if (a[i] == a[n-i-1])
            {
                m++;
                if (m==k)
                {
                    return true;
                }
            }
            else return false;
        }
    }
    return false;
}

int main() {
    int a[MAXN], n;
    NhapMang(a, n);
    if (is_MangDoiXung(a, n) == true)
        std::cout << "Mang doi xung.";
    else
        std::cout << "Mang khong doi xung.";
    return 0;
}
