#include <iostream>
#define MAXN 100

bool sohoanhao(int n)
{
    if (n <= 1)
        return false;

    long k = 1; // Start with 1 as every number is divisible by 1
    for (long i = 2; i * i <= n; i++)
    {
        if (n % i == 0)
        {
            k += i;
            if (i * i != n) // Avoid counting the same divisor twice for perfect squares
                k += n / i;
        }
    }
    return k == n;
}

void Input(int a[], int &n)
{
    std::cin >>n;
    for (int i=0; i<n; i++)
    {
        std::cin >> a[i];
    }
}

long TongCacSoHoanHao(int a[], int n)
{
    if (n==0) return 0;
    long S=0;
    for (int i=0; i<n; i++)
    {
        if (sohoanhao(a[i])) S+=a[i];
    }
    return S;
}

int main() {
    //int n;
   // std::cin >> n;
    int a[MAXN], n;
    Input(a, n);
  //  std::cout << std::boolalpha<< sohoanhao(n);
    std::cout << TongCacSoHoanHao(a, n);
    return 0;
}
