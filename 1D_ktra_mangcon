//###INSERT CODE HERE -
#include <iostream>
#include <string.h>
#define MAXN 100
void NhapMang (int a[], int &n)
{
    std::cin >>n;
    for (int i=0; i<n;i++)
    {
        std::cin >> a[i];
    }
}

bool is_MangCon(int a[], int &na, int b[], int &nb)
{
    int n1=0, n2=0, k=0;
    if (na==0) return true;
    while (n1<na && n2<nb)
    {
        if (a[n1]!=b[n2])
        {
            n2++;
        }
        else if (a[n1]==b[n2])
        {
            k++;
            n1++;
            n2++;
        }
    }
    if (k==na) return true;
    return false;
}

int main() {
    int a[MAXN], na, b[MAXN], nb;
    NhapMang(a, na);
    NhapMang(b, nb);
    if (is_MangCon(a, na, b, nb) == true)
        std::cout << "true";
    else
        std::cout << "false";
    return 0;
}
