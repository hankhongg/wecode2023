#include <iomanip>
#include <iostream>
#include <stdio.h>
using namespace std;
#define MAXN 100

struct SinhVien {
    char *MASV;
    char *HoTen;
    char NgaySinh[12];
    char GioiTinh;
    float DiemToan, DiemLy, DiemHoa, DTB;
};

//###INSERT CODE HERE -
void NhapC(SinhVien &a)
{
    a.MASV = new char[MAXN];
    cin.getline(a.MASV, MAXN);
    a.HoTen = new char[MAXN];
    cin.getline(a.HoTen, MAXN);
    cin.getline(a.NgaySinh, MAXN);
    cin >> a.GioiTinh >> a.DiemToan >> a.DiemLy >> a.DiemHoa;
    cin.ignore();
}

void XuatC(SinhVien &a)
{
    cout << a.MASV << "\t" << a.HoTen << "\t" << a.NgaySinh <<"\t" << a.GioiTinh << "\t"  << defaultfloat << a.DiemToan << "\t" << a.DiemLy << "\t" << a.DiemHoa
    << "\t";
    cout <<  setprecision(3) << (a.DiemToan+a.DiemLy+a.DiemHoa)/3 << endl; // << a.HoTen << "\t";
}
void Nhap(SinhVien *&a, int &n)
{
    cin >> n;
    a = new SinhVien[MAXN];
    cin.ignore();
    for (int i=0; i<n; i++)
    {
        NhapC(a[i]);
    }
}

void Xuat(SinhVien *&a, int n)
{
    for (int i=0; i<n; i++)
    {
        XuatC(a[i]);
    }
}

int main() {
    SinhVien *A;
    int n;
    Nhap(A, n);
    Xuat(A, n);
    return 0;
}
