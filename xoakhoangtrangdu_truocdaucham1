#include <iostream>
#include <string.h>
using namespace std;
#define MAX 300

void XoaKhoangTrangDu_TruocDauCham(char *s)
{
    int k=0;
    while (s[k]!='\0') k++;

    for (int i=0; i<k; i++)
    {
        if (s[i]==' ')
        {
            if (s[i+1]== '.')
            {
                while (s[i+1]!='\0')
                {
                    s[i]=s[i+1];
                    i++;
                }
                s[k-1]=NULL;
                XoaKhoangTrangDu_TruocDauCham(s);
            }
        }
    }
}

int main() {
	char s[MAX];
	cin.getline(s, MAX);
	cout << s << endl;
	char s1[MAX];
	strcpy(s1, s);
	XoaKhoangTrangDu_TruocDauCham(s1);
	cout << s1 << endl;
	return 0;
}
