
#include <iostream>

//###INSERT CODE HERE -
#define MAXN 100

bool sohoanhao(int &n)
{
    long k=0;
    for (long i=1; i<=n/2; i++)
    {
        if (n%i==0)
        {
            k+=i;
        }
    }
    if (k==n) return true;
    else return false;
}

void Input(int a[], int &n)
{
    std::cin >>n;
    for (int i=0; i<n; i++)
    {
        std::cin >> a[i];
    }
}

long TongCacSoHoanHao(int a[], int n)
{
    if (n==0) return 0;
    long S=0;
    for (int i=0; i<n; i++)
    {
        if (sohoanhao(a[i])) S+=a[i];
    }
    return S;
}

int main() {
    //int n;
   // std::cin >> n;
    int a[MAXN], n;
    Input(a, n);
  //  std::cout << std::boolalpha<< sohoanhao(n);
    std::cout << TongCacSoHoanHao(a, n);
    return 0;
}
