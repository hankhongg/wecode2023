//###INSERT CODE HERE -
#include <iostream>

#define MAXN 100
using namespace std;

int Input()
{
    int n;
    cin >> n;
    return n;
}

void NhapMang(int a[], int &n)
{
    for (int i=0; i<n; i++)
    {
        cin >> a[i];
    }
}

void doicho(int &a, int &b)
{
    int t=a;
    a=b;
    b=t;
}

void SapXepSoChanGiamDan(int a[], int &n)
{
    int b[MAXN], c[MAXN];
    int nb=0, j=0, k=0;
    int nc=0, m=0;
    for (int i=0; i<n; i++)
    {
        if (a[i]%2==0 && a[i]!=0)
        {
            b[nb++]=a[i];
        }
        else
        {
            c[nc++] = a[i];
        }
    }
    for (int i=0; i<nb-1;i++)
    {
        for (int j=i+1; j<nb; j++)
        {
            if (b[i]<b[j]) doicho(b[i], b[j]);
        }
    }
    for (int i=0; i<nc-1; i++)
    {
        if (c[i] < c[i+1]) doicho(c[i], c[i+1]);
    }

    while (k<nb && j<nb)
    {
        a[j++] = b[k++];
    }
    while (j<n && m<nc)
    {
        a[j++] = c[m++];
    }

}

void XuatMang(int A[], int N) {
    for (int i = 0; i < N; i++)
        std::cout << A[i] << '\t';
}
int main() {
    int a[MAXN], n;
    n=Input();
    NhapMang(a, n);
    SapXepSoChanGiamDan(a, n);
    XuatMang(a, n);
    return 0;
}
