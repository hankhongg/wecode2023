#include <iostream>
#define MAX 100

using namespace std;

struct Point
{
    int a;
    int b;
};

void NhapD(Point &a)
{
    cin >> a.a >> a.b;
}

void XuatD(Point &a)
{
    cout << "(" << a.a << ", " << a.b << ")" << endl;
}

void Nhap(Point *&a, int &n)
{

    cin >> n;
    a = new Point[n];
    for (int i=0; i<n; i++)
    {
        NhapD(a[i]);
    }
}

void Xuat(Point* a, int &n)
{
    for (int i=0; i<n; i++)
    {
        XuatD(a[i]);
    }
}

int main() {
    Point *points;
    int n;
    Nhap(points, n);
    Xuat(points, n);
    return 0;
}
